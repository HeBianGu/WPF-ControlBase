<UserControl x:Class="HeBianGu.Application.ManagerWindow.View.Float.CircleControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HeBianGu.Application.ManagerWindow.View.Float"
             xmlns:h="QQ:908293466"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    
    <ScrollViewer>
        <Grid >
            <GroupBox Header="CirlePanel">
                <UniformGrid Columns="3">
                    <h:FButton Content="Circle" Command="{Binding RelayCommand}" 
                               CommandParameter="{StaticResource S.Window.Float.Circle}"/>

                    <h:FButton Content="Arc" Command="{Binding RelayCommand}" 
                               CommandParameter="{StaticResource S.Window.Float.Arc}"/>

                    <h:FButton Content="Default" Command="{Binding RelayCommand}" 
                               CommandParameter="{StaticResource S.Window.Float.StackPanel}"/>

                    <h:FButton Content="TopLeft" Command="{Binding RelayCommand}" 
                               CommandParameter="{StaticResource S.Window.Float.StackPanel.TopLeft}"/>

                    <h:FButton Content="TopRight" Command="{Binding RelayCommand}" 
                               CommandParameter="{StaticResource S.Window.Float.StackPanel.TopRight}"/>

                    <h:FButton Content="BottomLeft" Command="{Binding RelayCommand}" 
                               CommandParameter="{StaticResource S.Window.Float.StackPanel.BottomLeft}"/>
                </UniformGrid>
            </GroupBox>
        </Grid>
    </ScrollViewer>
</UserControl>
