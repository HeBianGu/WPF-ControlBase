        NotifyIconSource="/HeBianGu.General.WpfControlLib;component/Resources/iBook.ico"
        mc:Ignorable="d" Title="ShellWindow"  Height="680" Width="1166">
    
    <h:LinkWindowBase.Logo>
        <BitmapImage UriSource="/HeBianGu.General.WpfControlLib;component/Resources/logo.png"/>
    </h:LinkWindowBase.Logo>

    <h:LinkWindowBase.TabLinks>
        <h:TabLink DisplayName="我的网盘" Controller="Loyout" Logo="{x:Null}" Action="Home"/>
        <h:TabLink DisplayName="传输列表" Controller="Custom" Action="Home"/>
        <h:TabLink DisplayName="好友分享" Controller="Custom" Action="Home"/>
        <h:TabLink DisplayName="功能宝箱" Controller="Custom" Action="Home"/>
        <h:TabLink DisplayName="找资源"  Controller="Custom" Action="Home"/>

    </h:LinkWindowBase.TabLinks>
    <Grid>
        
    </Grid>
</h:LinkWindowBase>
